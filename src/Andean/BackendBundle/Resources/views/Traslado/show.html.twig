{% extends '::backend.html.twig' %}

{% block id 'traslado' %}
{% block title %}Datos de la traslado {{ entity }}{% endblock %}

{% block content %}

<div class="input_wrapper">
  Nombre: <strong>{{ entity.nombre }}</strong>
</div>
<div class="input_wrapper">
  Precio: <strong>{{ entity.precio }}</strong>
</div>

{% endblock %}
{% block button_area %}
<a href="{{ path('backend_traslado_edit', { 'id': entity.id }) }}" class="button"><div class="icon"></div>Modificar Traslado</a>
<a href="{{ path('backend_traslado') }}" class="button"><div class="icon back"></div>Volver al listado</a>
<a class="button_primary button continue" href="{{ path('backend_traslado_new') }}">
  AÃ±adir un nuevo traslado
</a>
{% endblock %}