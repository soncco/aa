{% extends '::backend.html.twig' %}

{% block id 'trentipo' %}
{% block title %}Añadir un nuevo tipo{% endblock %}

{% block content %}
{{ form_errors(form) }}
<form action="{{ path('backend_trentipo_create') }}" method="post" {{ form_enctype(form) }}>
    <div class="input_wrapper">
    {{ form_row(form.numero) }}
    {{ form_row(form.empresa) }}
    {{ form_row(form.nombre) }}
    </div>
    <p>
        <button class="button_primary button continue" type="submit">Añadir tipo</button>
    </p>
    {{ form_rest(form) }}
</form>
{% endblock %}
{% block button_area %}
<a href="{{ path('backend_trentipo') }}" class="button"><div class="icon back"></div>Volver al listado</a>
{% endblock %}
